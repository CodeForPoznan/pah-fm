    "array.prototype.flatmap": "^1.2.2",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.12",
    "core-js": "^3.4.3",
    "date-fns": "1.28.5",
    "js-md5": "^0.7.3",
    "jwt-decode": "^2.2.0",
    "register-service-worker": "^1.5.2",
    "vue": "2.5.17",
    "vue-burger-menu": "^1.0.9",
    "vue-flag-icon": "^1.0.6",
    "vue-i18n": "^8.3.2",
    "vue-router": "3.0.1",
    "vue-select": "^3.2.0",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.4.0",
    "vuex-persistedstate": "^2.5.4"
