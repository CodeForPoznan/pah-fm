    "@vue/eslint-config-airbnb": "3.0.5",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.2.4",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.0.1",
    "http-server": "^0.11.1",
    "jest": "^25.2.4",
    "node-sass": "7.0.0",
    "sass-loader": "7.0.1",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "2.5.17"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
