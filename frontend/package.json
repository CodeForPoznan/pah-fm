    "@vue/eslint-config-airbnb": "5.0.2",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.4",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "http-server": "^0.12.3",
    "jest": "^25.2.4",
    "node-sass": "4.14.0",
    "sass-loader": "8.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-processor-html": "1.0.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "2.6.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
