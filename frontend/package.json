    "bootstrap-vue": "2.13.1",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.5",
    "date-fns": "^2.13.0",
    "js-md5": "^0.7.3",
    "jwt-decode": "^2.2.0",
    "register-service-worker": "^1.7.1",
    "vue-burger-menu": "^1.0.9",
    "vue-flag-icon": "^1.0.6",
    "vue-i18n": "^8.17.4",
    "vue-router": "3.1.6",
    "vue-select": "^3.10.3",
    "vue": "2.6.11",
    "vuex-map-fields": "^1.4.0",
    "vuex": "^3.3.0",
    "vuex-persistedstate": "^3.0.1"
