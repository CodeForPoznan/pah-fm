{
   "processors": ["stylelint-processor-html"],
  "extends": "stylelint-config-standard",
  "rules": {
      "at-rule-no-unknown": [true, {
        "ignoreAtRules": ["if", "else", "mixin", "include"]
      }],
      "selector-class-pattern": "^([a-z][a-z0-9]*)(-[a-z0-9]+)*$",
      "no-empty-source": null
    }
}
